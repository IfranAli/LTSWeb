{
  "name": "ltsweb",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "analyze": "ng build --stats-json && npx webpack-bundle-analyzer dist/ltsweb/stats.json",
    "test": "jest",
    "depcheck": "npx depcheck",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "buildCSS": "sass --load-path=./ -I ./node_modules src/styles/styles.scss dist/styles_out.css"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.1.1",
    "@angular/cdk": "18.1.1",
    "@angular/common": "18.1.1",
    "@angular/compiler": "18.1.1",
    "@angular/core": "18.1.1",
    "@angular/forms": "18.1.1",
    "@angular/platform-browser": "18.1.1",
    "@angular/platform-browser-dynamic": "18.1.1",
    "@angular/router": "18.1.1",
    "rxjs": "7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "0.14.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.1.1",
    "@angular-devkit/core": "18.1.1",
    "@angular-devkit/schematics": "18.1.1",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "18.1.1",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@compodoc/compodoc": "^1.1.19",
    "@schematics/angular": "18.1.1",
    "@types/jest": "^29.2.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.2.1",
    "babel-loader": "9.1.3",
    "jest": "^29.2.1",
    "jest-preset-angular": "14.1.1",
    "postcss": "^8.4.39",
    "tailwindcss": "3.4.6",
    "ts-jest": "^29.0.3",
    "typescript": "5.5.3",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^5.0.4"
  }
}
